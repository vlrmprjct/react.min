        "@types/react": "^18.2.58",
        "@types/react-dom": "^18.2.19",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.10.0",
        "css-minimizer-webpack-plugin": "^6.0.0",
        "dotenv": "^16.4.5",
        "dotenv-webpack": "^8.0.1",
